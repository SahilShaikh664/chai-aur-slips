using System;


class fruit
{
    protected int count;

    public fruit(int count)
    {
        this.count = count;
    }
    public int Getcount()
    {
        return count;
    }
}

class Apple:fruit
{
    public Apple(int count) : base(count) { }
}

class Mangoes:fruit
{
    public Mangoes(int count) : base(count) { }
}

class Program
{
    public static void Main(string[] args)
    {
        Mangoes man = new Mangoes(10);
        Apple app = new Apple(10);

        int total = app.Getcount() + man.Getcount();

        Console.WriteLine("Total Number Of Fruit" + total);
        Console.ReadLine();
    }
    
}
