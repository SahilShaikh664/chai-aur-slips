Module Module1

    Sub Main()
        Dim input As String

        Console.WriteLine("Enter The Character")
        input = Console.ReadLine()

        If "AEIOUaeiou ".Contains(input) Then
            Console.WriteLine("Character Is Vowel" + input)
        Else
            Console.WriteLine(input + "It A Consonant")
        End If

        If Char.IsUpper(input) Then
            Console.WriteLine("This is UpperCase" + input)

        Else
            Console.WriteLine("this Is Lowercase" + input)

        End If

        Console.ReadLine()




    End Sub

End Module
